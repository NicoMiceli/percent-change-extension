{
  "name": "Percent Change Calculator",
  "version": "0.0.2",
  "description": "Quick and simple way to calculate percent changes.",
  "browser_action": {
    "default_title": "Calculate Some Percentages!",
    "default_icon": "percent-change-icon19.png",
    "default_popup" : "popup.html"
  },
  "icons": {
    "48": "percent-change-icon48.png",
    "128": "percent-change-icon128.png"
  },
  "author": "@NicoMiceli",
  "permissions": ["storage"],

  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
